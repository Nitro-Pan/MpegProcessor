        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MpegProcessingWindow"
        mc:Ignorable="d"
        Title="playback" Height="450" Width="800">
    <DockPanel Name="MainGrid">
        <Menu DockPanel.Dock ="Top">
